<AnchorPane prefHeight="195.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeFormController">
   <children>
      <Pane prefHeight="195.0" prefWidth="614.0" style="-fx-background-color: blue;">
