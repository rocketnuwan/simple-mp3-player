            <Label fx:id="lblSong" alignment="CENTER" layoutX="25.0" layoutY="18.0" prefHeight="68.0" prefWidth="562.0" style="-fx-border-color: black; -fx-border-width: 5; -fx-border-radius: 10;" text="MP3 Player" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="29.0" />
               </font>
            </Label>
            <ProgressBar fx:id="prgSong" layoutX="26.0" layoutY="93.0" prefHeight="21.0" prefWidth="562.0" progress="0.0" style="-fx-background-color: white;" />
            <Button fx:id="btnPlay" layoutX="25.0" layoutY="126.0" mnemonicParsing="false" onAction="#btnPlayOnAction" prefHeight="52.0" prefWidth="75.0" style="-fx-background-color: white;" text="Play" />
            <Button fx:id="btnPause" layoutX="106.0" layoutY="126.0" mnemonicParsing="false" onAction="#btnPauseOnAction" prefHeight="52.0" prefWidth="75.0" style="-fx-background-color: white;" text="Pause" />
