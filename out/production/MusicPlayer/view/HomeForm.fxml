            <ComboBox fx:id="cmbSpeed" layoutX="447.0" layoutY="146.0" onAction="#cmbChangeSpeedOnAction" prefHeight="30.0" prefWidth="140.0" promptText="Speed" style="-fx-background-color: white;" />
            <Slider fx:id="sideVolume" layoutX="447.0" layoutY="123.0" style="-fx-background-color: red;" value="50.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
